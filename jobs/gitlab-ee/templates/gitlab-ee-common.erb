OMNIBUS_PACKAGE=/var/vcap/packages/gitlab-ee-omnibus/opt/gitlab
GITLAB_JOB_DIR=/var/vcap/jobs/gitlab-ee
OPT_DIR=/opt/gitlab
MONITOR_FILE=$GITLAB_JOB_DIR/initialized
GITLAB_STORE=/var/vcap/nfs/shared
DB_INITIALIZED_FILE=$GITLAB_STORE/db_initialized
LOG_DIR=/var/vcap/sys/log/gitlab
TMPDIR=/var/vcap/data/gitlab-ee/tmp

# Pre create log directory
mkdir -p $LOG_DIR

# Fix sys and log permissions
chmod o+x /var/vcap/sys /var/vcap/sys/log
